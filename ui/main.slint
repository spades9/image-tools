import { Button } from "std-widgets.slint";
import { Home } from "pages/home.slint";
import { CompressGlobal } from "./model/compress_global.slint";

export { CompressGlobal }

export component HelloWorld inherits Window{
    //窗体图标
    icon: @image-url("./assets/icon/logo.png");
    //窗体名称
    title: "图像处理";
    //点击处理方法
    callback execute();
    callback clicked(int);
    callback compress_open();
    //显示文本
    in property <string> name: "默认";
    in-out property <length> font_size: 12px;
    default-font-size: font_size;
    
    //窗体最小尺寸
    min-width: 900px;
    min-height: 600px;

    max-width: 1200px;
    max-height: 800px;

    Home { 
        clicked(e) => {
            clicked(e);
        }
        compress_open => {
            compress_open();
        }
    }


    
}